<base:ApplicationBase x:TypeArguments = "views:MainWindow, main:MainViewModel"
                      x:Class = "OrionManager.App"
                      xmlns:base = "clr-namespace:Senticode.Wpf.Base;assembly=Senticode.Wpf"
                      xmlns = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x = "http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:main = "clr-namespace:OrionManager.ViewModel.ViewModels.Main;assembly=OrionManager.ViewModel"
                      xmlns:views = "clr-namespace:OrionManager.Views"
                      x:ClassModifier = "internal">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source = "pack://application:,,,/Theme/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</base:ApplicationBase>