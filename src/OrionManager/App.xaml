<base:ApplicationBase x:TypeArguments="orionManager:AppSettings, orionManager:AppCommands"
                      x:Class="OrionManager.App"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:base="clr-namespace:Senticode.Tools.WPF.Core.Base;assembly=Senticode.Tools.WPF.Core"
                      xmlns:orionManager="clr-namespace:OrionManager"
                      x:ClassModifier="internal">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary
                    Source="pack://application:,,,/OrionManager;component/Views/Resources/RootDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</base:ApplicationBase>