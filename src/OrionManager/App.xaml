<base:ApplicationBase x:Class = "OrionManager.App"
                      x:TypeArguments = "viewModel:AppSettings, viewModel:AppCommands"
                      xmlns:base = "clr-namespace:Senticode.Wpf.Base;assembly=Senticode.Wpf"
                      xmlns = "http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x = "http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:viewModel = "clr-namespace:OrionManager.ViewModel;assembly=OrionManager.ViewModel"
                      x:ClassModifier = "internal">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source = "pack://application:,,,/Theme/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</base:ApplicationBase>